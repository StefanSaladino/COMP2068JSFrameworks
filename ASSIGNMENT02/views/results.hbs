
  <h1>{{title}}</h1>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Address</th>
        <th>Status</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      {{#each restaurants}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.vicinity}}</td>
        <td>{{this.business_status}}</td>
        <td></td>
      </tr>
      {{/each}}
    </tbody>
  </table>

