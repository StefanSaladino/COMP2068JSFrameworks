  <h1>{{title}}</h1>

<section class="user-cards">
<div class="card">
  <h5 class="card-header">User Info</h5>
  <div class="card-body">
    <h5 class="card-title">Username</h5>
    <p class="card-text">{{user.username}}</p>
    <h5 class="card-title">Role</h5>
    <p class="card-text">{{role}}</p>
    <h5 class="card-title">Status</h5>
    <p class="card-text">{{status}}</p>
  </div>
</div>

<div class="card">
  <h5 class="card-header">Insights</h5>
  <div class="card-body">
    <h5 class="card-title">Last Sign In Time</h5>
    <p class="card-text">{{lastSignIn}}</p>
    <h5 class="card-title">Number of API Calls</h5>
    <p class="card-text">{{apiCalls}}</p>
    <h5 class="card-title">Number of Favourited Spots</h5>
    <p class="card-text">{{favourites}}</p>
    <h5 class="card-title">Verified Account</h5>
    <p class="card-text">{{verifiedAccount}}</p>
  </div>
</div>
</section>
